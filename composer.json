{
    "name": "ssnukala/sprinkle-crud6-vue",
    "type": "userfrosting-sprinkle",
    "description": "Generic CRUD Vue frontend sprinkle for UserFrosting 6.0 using sprinkle-crud6 as backend API",
    "keywords": [
        "php user management",
        "userfrosting",
        "sprinkle",
        "CRUD",
        "Vue",
        "frontend",
        "JSON schema"
    ],
    "homepage": "https://github.com/ssnukala/sprinkle-crud6-vue",
    "license": "MIT",
    "authors": [
        {
            "name": "Srinivas Nukala",
            "homepage": "https://srinivasnukala.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "userfrosting/framework": "^6.0",
        "userfrosting/sprinkle-core": "^6.0",
        "userfrosting/sprinkle-account": "^6.0",
        "userfrosting/sprinkle-admin": "^6.0",
        "ssnukala/sprinkle-crud6": "^0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.1",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.2",
        "fakerphp/faker": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\CRUD6Vue\\": "app/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\CRUD6Vue\\Tests\\": "app/tests/"
        }
    }
}